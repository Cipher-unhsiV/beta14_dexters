<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Public Form</title>
    <link rel="stylesheet" href="../css/bootstrap.min.css" />
    <link rel="stylesheet" href="../css/styles.css" />
    <style>
      table,
      th,
      td {
        border: 1px solid black;
      }
    </style>
  </head>
  <body>
    <div class="container">
      <div class="wrapper-container">
        <h1 class="text-center margin-bottom">List of Donors</h1>
        <table style="width: 100%" class="margin-bottom">
          <tr class="text-center">
            <th>Username</th>
            <th>Name</th>
            <th>Email</th>
            <th>Phone</th>
            <th>No of People served</th>
          </tr>
          <% for(let value of userDetails){ %>
          <tr class="text-center">
            <td><%= value.username %></td>
            <td><%= value.name %></td>
            <td><%= value.email %></td>
            <td><%= value.phone %></td>
            <td>50</td>
          </tr>
          <% } %>
        </table>
        <a href="/superadmin/dashboard">Go back to Dashboard</a>
      </div>
    </div>
  </body>
</html>
